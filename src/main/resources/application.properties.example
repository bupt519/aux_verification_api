server.port=8080

spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
spring.datasource.url=jdbc:mysql://ip:port/db?charset=utf8mb4&useSSL=false&serverTimezone=Asia/Shanghai
spring.datasource.username=
